{
    "name": "kiner/philippines-holidays",
    "description": "A package for fetching Philippines holidays",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Kiner\\PhilippinesHolidays\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kiner\\PhilippinesHolidays\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kiner\\PhilippinesHolidays\\PhilippinesHolidaysServiceProvider"
            ]
        }
    }
}
